<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
<listeners>
<listener class-name="reports.TestListeners"/>
<listener class-name="reports.ExtentReporterNG"/>
</listeners>
	<test name="Invalid Login Test">

		<classes>
			<class name="com.w2a.appium.testcases.Test_InvalidLogin" />
		</classes>
	</test> <!-- Test -->
	<test name="Valid Login Test">

		<classes>
			<class name="com.w2a.appium.testcases.Test_ValidLogin" />
		</classes>
	</test> <!-- Test -->
	<test thread-count="5" name="Product Listing Test">
		<classes>
			<class name="com.w2a.appium.testcases.Test_ProductListing" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
